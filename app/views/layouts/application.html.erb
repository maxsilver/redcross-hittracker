<!DOCTYPE html>
<html>
<head>
  <title>HitTracker</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>


  <%= link_to "Dashboard", root_path %>
  <%= link_to "Users", users_path %>
  <br /><br />
  <%= link_to "Hits", hits_path %>
