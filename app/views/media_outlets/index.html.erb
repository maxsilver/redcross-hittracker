Media INDEX!

<div>
  <% form_for :media do |f| %>

    <span>Time :</span>
    <%= f.text_field :start_time %>
  
    <%= f.label :end_time %>
    <%= f.text_field :end_time %>  
  
    <%= f.submit %>
  <% end %>
</div>

<h3>Reporters</h3>
<ul class="reporters">
  <% 5.times do |i| %>
    <li>
      <span class="name">Name</span>
      <span class="count"><%= i %></span>
      <span class="button"><a href="arrow">=></a></span>
    </li>
  <% end %>
</ul>

<h3>Outlets</h3>
<ul class="outlets">
  <% 5.times do |i| %>
    <li>
      <span class="name">Name</span>
      <span class="count"><%= i %></span>
      <span class="button"><a href="arrow">=></a></span>
    </li>
  <% end %>
</ul>  